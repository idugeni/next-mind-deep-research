# Google API Keys
GOOGLE_SEARCH_API_KEY=your_google_search_api_key
GOOGLE_SEARCH_CX=your_google_search_cx

# Google Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Upstash Redis
UPSTASH_REDIS_REST_URL=your_upstash_redis_rest_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_rest_token

# Backend API Key
# Jika true, backend selalu memakai API key dari environment (GEMINI_API_KEY), user tidak perlu input API key di frontend. Lebih aman untuk production.
# Jika false, backend menerima API key dari user/frontend (mode demo/testing).
USE_BACKEND_API_KEY=true
